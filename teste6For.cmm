$TRACE_OFF

int i;

void main() {
    write("Loop for de 0 a 2");
    for (i = 0; i < 3; i++) {
        write("i = ", i);
    }
    
    write(" ");
    write("Loop for com partes vazias");
    i = 0;
    for ( ; i < 2; ) {
        write("i de novo = ", i);
        i++;
    }
    
    write("Fim.");
}