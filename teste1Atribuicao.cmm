$TRACE_OFF

int a;
int b;
int c;

void main() {
   write("Testando atribuicao encadeada...");
   a = b = 10;
   
   write("a deve ser 10: ", a);
   write("b deve ser 10: ", b);

   write(" ");
   write("Testando atribuicao como expressao...");
   
   write("c deve ser 25: ", c = a + 15);
   
   write("verificando c: ", c);
}