$TRACE_OFF

int a;

void main() {
    a = 5;
    write("Valor inicial de a: ", a);

    write("Valor da expressao a++: ", a++);
    write("Valor de a apos a++: ", a);

    write("Valor da expressao ++a: ", ++a);
    write("Valor de a apos ++a: ", a);
    
    write("Valor da expressao a--: ", a--);
    write("Valor de a apos a--: ", a);
    
    write("Valor da expressao --a: ", --a);
    write("Valor de a apos --a: ", a);
}